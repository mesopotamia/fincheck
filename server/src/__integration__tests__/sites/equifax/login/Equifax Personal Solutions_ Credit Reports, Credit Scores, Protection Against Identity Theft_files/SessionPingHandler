<script>
	function updateCurrentCountry(site){	
		if (site == ""){
			site = "United States"
		}
		if(site == "España" && conSite){
			site = "Spain"
		}
		var site_css = site.replace(" ", ".");

		// Update Flag 
		$("li.nav-item-locale a img:first").attr("src" , $("#country-dropdown li." + site_css + " a img").attr("src"));
		$("li.nav-item-locale a img:first").attr("alt" , site);
		$("li.nav-item-locale a img:first").attr("title" , site);
		
		// Bring Country Up
		$("#country-dropdown li." + site_css).parent().prepend($("#country-dropdown li." + site_css));
	}
	var conSite = false;
</script>

<li class="dropdown nav-item-locale">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
