<script>
	function _GcsBingSearch_search() {
		var keywords = document._GcsBingSearch_fm._GcsBingSearch_keywords.value;		
		keywords = keywords.replace('Search Equifax Personal', '');
		keywords = keywords.replace(/^\s+|\s+$/, '');
		keywords = $.trim(keywords)
		// tracking search
		if (keywords != '' && keywords != 'Search Equifax Personal') {
			document._GcsBingSearch_fm.submit();
		}
	}
</script>

	
	
					
				
			
		
	
	


