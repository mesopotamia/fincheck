/* This function is created to filter the particular content to be displayed for ajax implementation
 *	in average loan rate finder portlet
 */

function rateFinderPostFunction() {
	var holder = document.getElementById("displayId");
	var data = holder.innerHTML;
	if (data == "") {
		return true;
	}
	var firstIndex = data.indexOf("AvgRateContentStart");
	var lastIndex = data.indexOf("AvgRateContentEnd");
	var firstInd = firstIndex + 22;
	var lastInd = lastIndex - 4;
	var rate_finder = data.substring(firstInd, lastInd);
	holder.innerHTML = rate_finder;
	return true;
}

function changeLoanTypeForLoanRateFinder(catVal) {
	//var catSel = document.getElementsByName("smtLoanCat");
	//catSel = catSel[0];
	var typeSel = document.getElementsByName("typeSelected");

	if (typeSel[0] == null)
		return;

	typeSel = typeSel[0];

	// Begin loan type arrays by category  (cat[0] = Mortgage), (cat[1] = Auto), (cat[2] = home equity)
	var cat = new Array();
	cat[0] = [ '30 yr Fixed Conforming', '30 yr Fixed Non-Conforming',
			'15 yr Fixed Conforming', '15 yr Fixed Non-Conforming',
			'1 yr ARM Conforming', '1 yr ARM Non-Conforming',
			'3 yr ARM Conforming', '3 yr ARM Non-Conforming',
			'5 yr ARM Conforming', '5 yr ARM Non-Conforming',
			'7 yr ARM Conforming', '7 yr ARM Non-Conforming',
			'10 yr ARM Conforming', '10 yr ARM Non-Conforming' ];
	cat[1] = [ '36 month New Auto', '48 month New Auto', '60 month New Auto',
			'48 month Used Auto' ];
	cat[2] = [ '10 yr Fixed HE Loan', '15 yr Fixed HE Loan',
			'HELOC $50,000 80% LTV', 'HELOC $50,000+ 80% LTV' ];
	// End loan type arrays by category
	// Option values
	var optVal = [ 'mort', 'auto', 'heloan' ];
	// Clear out previous options
	typeSel.options.length = 0;
	// Begin filling in new options
	//var catVal = catSel.options[catSel.selectedIndex].value;
	for ( var i = 0; i < cat[catVal].length; i++) {
		if (i < 9) {
			var increment = "0" + (i + 1);
		} else {
			var increment = i + 1;
		}
		typeSel.options[i] = new Option(cat[catVal][i], optVal[catVal]
				+ increment);
	}
	// End filling in new options
}

function removeAllOptions(selectbox) {
	var i;
	for (i = selectbox.options.length - 1; i >= 0; i--) {
		//selectbox.options.remove(i);
		selectbox.remove(i);
	}
}

function addOption(selectbox, value, text) {
	var optn = document.createElement("OPTION");
	optn.text = text;
	optn.value = value;
	selectbox.options.add(optn);
}

function submitScoreForm() {
	if (document.LoanTypeForm.typeSelected.value == "") {
		document.LoanTypeForm.typeSelected.focus();
		return false;
	}
	if (document.LoanTypeForm.stateSelected.value == "") {
		document.LoanTypeForm.stateSelected.focus();
		return false;
	}
	if (document.LoanTypeForm.LoanTypeCity.value == "") {
		document.LoanTypeForm.LoanTypeCity.focus();
		return false;
	}
	document.LoanTypeForm.submit();
}

