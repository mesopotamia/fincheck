function planSummaryPostFunction() {
	var holder = document.getElementById("plansummaryAjaxCall");
	var data = holder.innerHTML;
	if (data == "") {
		return true;
	}
	var firstIndex = data.indexOf("PlansumContentStart");
	var lastIndex = data.indexOf("PlansumContentEnd");
	var firstInd = firstIndex + 22;
	var lastInd = lastIndex - 4;
	var rate_finder = data.substring(firstInd, lastInd);
	holder.innerHTML = rate_finder;
	return true;
}
