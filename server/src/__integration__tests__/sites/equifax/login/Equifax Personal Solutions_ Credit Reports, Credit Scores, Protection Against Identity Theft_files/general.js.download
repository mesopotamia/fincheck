	return true;
}

function setLocale(locale){
	linkObject=document.getElementById("changeLocale");
	var url = window.location.href;
	 if(url.indexOf("_CA")!=-1){
	    url=url.replace("fr_CA",locale); 
	    url=url.replace("en_CA",locale); 
	 }
	 else
	 {
	  url = (url.indexOf("?")==-1)?url+"?":url+"&";	 
	  url=url+"locale_code="+locale;  
	 }
	  linkObject.href=url;
	}

/*Start:Added for family plan overlay*/
function submitAdultEnroll(baseURL) {
	if (!document.forms["memberForm"].termsCheck.checked) {
		return;
	}
	var submitURL;
	var ele;
	var i=0;
	var inputElements = new Array("Enter your name", "Enter your last name", "Email", "Confirm Email");
	for(i=0;i<inputElements.length;i++){
		ele = document.getElementById(inputElements[i]);
		if( $(ele).attr('title') == $(ele).val()){
			$(ele).val('');
		}
	}
	var firstName = encodeURIComponent(document.memberForm.firstName.value);
	var lastName = encodeURIComponent(document.memberForm.lastName.value);
	var email = encodeURIComponent(document.memberForm.email.value);
	var confirmEmail = encodeURIComponent(document.memberForm.confirmEmail.value);
	submitURL=baseURL+"&firstName=" + firstName + "&lastName="+ lastName + "&email=" + email + "&confirmEmail=" + confirmEmail;
	displayFamilyOverlay(submitURL);
}
function displayFamilyOverlay(submitURL) {
	var mytime = "&ts=" + new Date().getTime();
	var submitUrl = submitURL + mytime;
		$.ajax({
		type: "GET",
		url: submitUrl,
		cache: false,
		dataType: "html",
		success: function(html){
			var isValid = false;
			if (html.indexOf('familyplan_overlay') != -1){
				isValid = true;
			}				
			if(isValid){
				jQuery('#familyplan_overlay').replaceWith(html);
			}
			else{
				$('#familyplan_overlay').find('#fpCallWaits').hide();
				$('#familyplan_overlay').find('#fpCall').show();
				$('#fpCallWait').hide();
				$('#fpCall .ButtonRightMPMyProfile').show();
				document.memberForm.reset();
				$('input[title]').inputHints();
				/* blur effect start*/
				$('input[title]').each(function(){
					$(this).blur(function(){
						if ($(this).val() == '')
						$(this).val($(this).attr('title')).addClass('hint');
					});
				});
				/* blur effect end*/
				if (document.getElementById("ajaxError") != null) {
					$('#termsCheck').show();
					$('#orderComp').hide();										
					$('#ajaxError').show();
				}
			}				
		}		
	});
}
