function StateProductValidation(form) {

	var restrictedStatesString = form.restStatesProd.value;
	var prod_cd = form.prod_cd.value;
	if (restrictedStatesString != null) {
		var restrictedStatesArray = restrictedStatesString.split(',');

		var selectedState = form.state.options[form.state.selectedIndex].value;
		var flag = "FALSE";
		for (i = 0; i < restrictedStatesArray.length; i++) {
			if (restrictedStatesArray[i] == selectedState) {
				flag = "TRUE";
			}
		}
		if (flag == "TRUE") {
			var url = 'sitepage.ehtml?forward=state_restriction_warning&selectedState='
					+ selectedState + '&prod_cd=' + prod_cd;
			openPop(url);
		}
	}
}
